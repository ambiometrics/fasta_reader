{
    "name": "edwrodrig/fasta_reader",
    "description": "Library to read Fasta files",
    "authors": [
        {
            "name": "Edwin Rodríguez León",
            "email": "edwrodrig@gmail.com",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "keywords" : [
        "bioinformatics",
        "fasta",
        "oceanography"
    ],
    "license" : "MIT",
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.2"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^7.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\fasta_reader\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\fasta_reader\\" : "tests/"
        }
    },
    "archive": {
        "exclude": [
            "/tests",
            ".git",
            ".gitignore",
            "/.travis.yml",
            "/phpunit.xml.dist",
            "/examples",
            "/.idea",
            "README.md"
        ]
    }
}
